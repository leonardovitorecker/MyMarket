@model MyMarket.Models.Produto

@{
    ViewData["Title"] = "Details";
}
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - MyMarket</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/MyMarket.styles.css" asp-append-version="true" />
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-A3rJD856KowSb7dwlZdYEkO39Gagi7vIsF0jrRAoQmDKKtQBHUuLZ9AsSv4jD4Xa" crossorigin="anonymous"></script>
</head>
<body>
<h1>Details</h1>

<div>
    <h4>Produto</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.nomeProduto)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.nomeProduto)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.imagem)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.imagem)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.arquivo)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.arquivo)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.valorVenda)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.valorVenda)
        </dd>
      
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.categoriaid)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.categoriaid)
        </dd>
        
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.dataCadastro)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.dataCadastro)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.dataAlteracao)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.dataAlteracao)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.id">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
 <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
 
</body>
<div id="modal" class="modal fade" role="dialog" />
